
{
  "expo": {
    "name": "Essentials Only",
    "slug": "essentials-only",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/d6de2104-2eef-48cb-b764-28a7856fcd0d.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/d6de2104-2eef-48cb-b764-28a7856fcd0d.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.poppyseedla.essentialsonly",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/d6de2104-2eef-48cb-b764-28a7856fcd0d.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.poppyseedla.essentialsonly"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "essentialsonly",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
