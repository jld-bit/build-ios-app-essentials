
{
  "expo": {
    "name": "Survival Budget",
    "slug": "survival-budget",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/0cc5be7a-d009-4608-97c1-eead90b51cc9.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/0cc5be7a-d009-4608-97c1-eead90b51cc9.png",
      "resizeMode": "contain",
      "backgroundColor": "#4AAED9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.survivalbudget.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/0cc5be7a-d009-4608-97c1-eead90b51cc9.png",
        "backgroundColor": "#4AAED9"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.survivalbudget.app"
    },
    "web": {
      "favicon": "./assets/images/0cc5be7a-d009-4608-97c1-eead90b51cc9.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "survivalbudget",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
